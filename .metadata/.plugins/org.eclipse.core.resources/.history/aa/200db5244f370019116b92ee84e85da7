package conditionalstatements;

public class testing1 {

	public static void main(String[] args)
	{
		int a=10;
		int b=20;
		
		if(a>b)
		{
       System.out.println("a is greater then b");
	}else if (a==b)
	{	
		System.out.println("a is equal to b")	
	}
		else
	{
		System.out.println("b is greater then b");
	}

}
}