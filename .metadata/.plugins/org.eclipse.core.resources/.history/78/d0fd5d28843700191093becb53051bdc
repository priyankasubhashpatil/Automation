package variablesanddatatypes;

public class VariablesAndDataTypes {

	public static void main(String[] args)
	{
	int age=30;
	char gender='M';
    boolean married=true;
    double xyz=678.468;
    
    
    
    byte a=10;
    short b=40;
    long c=4999;
    float f=566.554f;
    
    String name="Priyanka"
    		
    String username="Priyanka123";
	
	System.out.Println(username);
	
	
    
	}

}
